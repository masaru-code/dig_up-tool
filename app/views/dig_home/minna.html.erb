<h1>みんなのやりたい事一覧</h1>
<%= form_with url: users_path,method: :get, local: true do |f| %>
  <%= f.label :name, '検索したいユーザー名' %>
  <%= f.text_field :name %>
  <%= f.submit "送信" %>
<% end %>

<% @users.each do |user| %>
  <%= if @menbers == @menber %>
    <% @tasks.each do |task| %>
      <li>
        <%= gravatar_for user, size: 50 %>
        <%= @menber %><%= task.name %>
        <%= link_to "チャットルームへ", "#", class: "btn btn-primary" %>
        <%= link_to "工程ページへ", "#", class: "btn btn-primary" %>
      </li>
   <% end %>
  <% end %>
<% end %>

