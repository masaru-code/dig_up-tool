<div class = "header fixed-top">
  <nav>
    <ul class="header-right">
      <h3>提案者:<%= @task.user&.name %></h3>
      <h2><%= @task.name %></h2>
      <li><%= link_to "sing-top", root_path %></li>
      <li><%= link_to "home", dig_home_dighome_path %></li>
      <li><%= link_to "sing_up", new_user_registration_path, class: "sing_css" %></li>
    </ul>
  </nav>
  <div class=”form-inline”>
    <%= form_with model: @coment, local: true do |form| %>
      <%= form.label :name, 'コメント入力' %>
      <%= form.text_field :name %>
      <%= form.file_field :image %>
      <%= form.submit "下に送信" %>
    <% end %>
  </div>
</div>